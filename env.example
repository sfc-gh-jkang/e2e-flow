# Prefect Worker Environment Configuration
# Copy this file to .env and replace these values with your actual credentials
# Run ./setup-prefect-env.sh to automatically create this file

# Prefect Cloud Configuration (REQUIRED)
PREFECT_API_URL=https://api.prefect.cloud/api/accounts/<your-account-id>/workspaces/<your-workspace-id>
PREFECT_API_KEY=pnu_your_api_key_here
PREFECT_WORK_POOL=spcs-process

# Application Settings
PYTHONUNBUFFERED=1
APP_PORT=8080
LOG_LEVEL=INFO

# Optional: Snowflake Connection (if needed by your flows)
# SNOWFLAKE_ACCOUNT=your-account-identifier
# SNOWFLAKE_USER=your-username
# SNOWFLAKE_PASSWORD=your-password
# SNOWFLAKE_WAREHOUSE=your-warehouse
# SNOWFLAKE_DATABASE=E2E_FLOW_DB
# SNOWFLAKE_SCHEMA=APP_SCHEMA
# SNOWFLAKE_ROLE=your-role